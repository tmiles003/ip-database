<h1>Editing static_ip</h1>

<% form_for(@static_ip) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </p>
  <p>
  	<%= f.label :outside_ip %><br />
	<%= f.text_field :outside_ip %>
  <p>
    <%= f.label :servername %><br />
    <%= f.text_field :servername %>
  </p>
  <p>
  	<%= f.label :description %><br />
	<%= f.text_area :description %>
  </p>
  <p>
    <%= f.label 'In Use' %><br />
    <%= f.check_box :available %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Back', static_ips_path %>
