<div id="edit">
	
<% form_for(@static_ip, :url => subnet_static_ip_path) do |f| %>
  <h1><%= @static_ip.address %></h1>
  <%= f.error_messages %>
  <p>
  	<%= f.label :outside_ip %><br />
	<%= f.text_field :outside_ip %>
  <p>
    <%= f.label :servername %><br />
    <%= f.text_field :servername %>
  </p>
  <p>
  	<%= f.label :description %><br />
	<%= f.text_area :description, :cols => 30, :rows => 4 %>
  </p>
  <p>
	<%= f.label :available, 'In Use' %><br />
	<%= f.check_box :available %>
  </p><br />
  <p>
    <%= f.submit "Update", {:style => 'float: right'} %>
  </p>
<%= link_to 'Back', subnet_static_ips_path %>
<% end %>


</div>