

<table>
  <tr>
    <th>Internal IP</th>
	<th>External IP</th>
    <th>Servername</th>
	<th>Description</th>
    <th>Available</th>
	<th>Online</th>
  </tr>

<% for static_ip in @static_ips %>
  <tr>
    <td><%= link_to static_ip.address, edit_static_ip_path(static_ip) %></td>
    <td><%=h static_ip.outside_ip%></td>
	<td width="200"><%=h format_AD_member_server(static_ip.servername) %></td>
	<td><%=h static_ip.description %></td>
    <td><%= format_available_status(static_ip.available) %></td>
	<td><%= format_online_status(static_ip.online) %></td>
  </tr>
<% end %>
</table>

<br />


